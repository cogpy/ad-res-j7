{
  "name": "ad-res-j7-workflow-tests",
  "version": "1.0.0",
  "description": "Validation tests for GitHub Actions workflows",
  "scripts": {
    "test": "node tests/run-all-tests.js",
    "test:validation": "node tests/workflow-validation.test.js",
    "test:integration": "node tests/integration-test.js",
    "test:api": "node tests/api-integration-tests.js",
    "test:comprehensive": "node tests/comprehensive-workflow-test.js",
    "test:security": "node tests/security-validation-test.js",
    "test:end-to-end": "node tests/end-to-end-workflow-test.js",
    "test:simple": "node tests/simple-workflow-test.js",
    "test:monitoring": "node tests/monitoring-test.js",
    "test:timeline": "node tests/timeline-integration-test.js",
    "test:duplicate-cleanup": "node tests/duplicate-cleanup-test.js",
    "test:label-handling": "node tests/label-handling-test.js",
    "test:label-creation": "node tests/manual-test-label-creation.js",
    "test:verbose": "node tests/run-all-tests.js --verbose",
    "test:hypergraph": "node tests/hypergraphql.test.js",
    "test:ad-hypergraph": "node tests/ad-hypergraph-test.js",
    "test:date-validation": "node tests/date-validation-test.js",
    "test:empty-todo-validation": "node tests/empty-todo-file-validation.test.js",
    "validate-dates": "python3 scripts/validate_analysis_dates.py",
    "hypergraph:example": "node docs/models/hypergnn/case-hypergraph.js",
    "validate-workflows": "node tests/run-all-tests.js",
    "lint": "echo 'No linting configured - using workflow validation instead' && npm run test:validation"
  },
  "dependencies": {
    "glob": "^11.0.3"
  },
  "keywords": [
    "github-actions",
    "workflow",
    "validation",
    "tests"
  ],
  "author": "GitHub Actions Workflow Validator",
  "license": "MIT"
}
